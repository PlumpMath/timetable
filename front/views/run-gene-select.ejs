<!DOCTYPE html>
<html lang="ko">
  <%- include head.ejs %>
  <script>var id='<%=id%>'</script>
  <body>
    <%- include nav.ejs %>
    <div id="content" class="container">
      <div class="container">
        <div class="row row-menu">
            <ol class="breadcrumb">
                <li><a href="/run-list/">결과 목록</a></li>
                <li ><a href="/run-detail/<%=id%>"><%=id%></a></li>
                <li class="active"><a href="#">세대 선택</a></li>
            </ol>
        </div>
        <div class="row">
            <h1><%=title%></h1>
        </div>
        <p></p>
        <div class="row">
            <h3>총 <%=info['gens']%>세대</h3>
        </div>
        <%for(var i=1;i<=Number(info['gens']);i++) {%>
            <%if(i%12==1) {%><div class="row"><%}%>
                <div class="col col-md-1">
                    <button type="button" class="btn btn-primary btn-select" onclick="location.href='<%='/run-gene/'+id+'/'+i%>';"><%=i%>세대</button>
                </div>
            <%if(i%12==0) {%></div><%}%>
        <%}%>
    </div>
    <%- include tail.ejs %>
  </body>
</html>
